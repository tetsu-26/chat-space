<header class="header">
  <div class="header-top">
    <p class="create-group">グループを作成しました</p>
  </div>
  <div class="header-in">
    <div class="header-left">
      <div class="in-lefthead">
        <div class="name-tag">
          <p class="user-name">user_name</p>
          <a href="#">
            <i class="fa fa-cog"></i>
          </a>
          <a href="#">
            <i class="fa fa-edit"></i>
          </a>
        </div>
      </div>
    </div>
    <div class="header-right">
      <div class="in-righthead">
        <p class="group-name">group_name</p>
        <p class="group-member">group_member</p>
        <a href="edit">
          <div class="edit-name">Edit</div>
        </a>
      </div>
    </div>
  </div>
</header>
<div class="contents">
  <div class="left-content">
    <div class="in-leftcontent">
      <div class="group">
        <p class="group-name">group-name</p>
        <p class="group-comment">group-comment</p>
      </div>
    </div>
  </div>
  <div class="contents-main">
    <div class="in-rightcontent">
      <div class="submit-table">
        <ul class="submit-record">
          <li class="submit-name">submit-user</li>
          <li class="submit-date">submit-date</li>
        </ul>
        <p class="submit-text">submit-text</p>
      </div>
    </div>
    <footer class="footer">
      <form class="submit-form">
        <div class="message-form">
          <input class="text-box" type-"text" name="messaage" placeholder="MESSAGE">
          <a href="#">
            <i class="fa fa-image"></i>
          </a>
        </div>
        <a href="#">
          <input class="submit-button" type="submit" value="Send">
        </a>
      </form>
    </footer>
  </div>
</div>
